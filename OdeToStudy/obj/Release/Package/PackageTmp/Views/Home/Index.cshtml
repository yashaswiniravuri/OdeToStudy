@model IEnumerable<OdeToStudy.Data.Models.Books>


@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <div>
        There are @Model.Count() books in the database.
    </div>
    <div>
        <ul>
            @foreach (var MyBooks in Model)
            {
                <li>
                    @MyBooks.Name
                </li>
            }
        </ul>
    </div>
</div>
